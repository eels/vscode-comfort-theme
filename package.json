{
  "name": "vscode-comfort-theme",
  "version": "0.0.0",
  "description": "A comforting dark theme, for me and for you",
  "type": "module",
  "license": "MIT",
  "author": "Liam Howell <liam@liam.codes>",
  "publisher": "",
  "displayName": "Comfort Theme",
  "icon": "resources/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/eels/vscode-comfort-theme"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "nodemon --watch src --exec yarn build",
    "i": "yarn install --ignore-engines",
    "lint": "yarn run lint:js:fix",
    "lint:js": "node_modules/.bin/eslint {scripts,src}/*.js --config .eslintrc.json",
    "lint:js:fix": "node_modules/.bin/eslint {scripts,src}/*.js --config .eslintrc.json --fix",
    "lint:theme": "node scripts/lint.js"
  },
  "files": [
    "themes/comfort.json"
  ],
  "keywords": [
    "color-theme",
    "comfort",
    "dark",
    "theme"
  ],
  "devDependencies": {
    "color": "^3.1.3",
    "eslint": "^7.23.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "eslint-plugin-sort-keys-fix": "^1.1.0",
    "eslint-plugin-standard": "^4.0.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "vsce": "^1.87.0"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Comfort",
        "path": "themes/comfort.json",
        "uiTheme": "vs-dark"
      }
    ]
  },
  "galleryBanner": {
    "color": "#292D3F",
    "theme": "dark"
  },
  "__metadata": {
    "publisherDisplayName": "Liam Howell"
  }
}
